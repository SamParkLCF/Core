<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net5.0</TargetFramework>
		<AssemblyName>LCF.Core</AssemblyName>
		<RootNamespace>LCF.Core</RootNamespace>
		<Version>1.0.0.0</Version>
		<Authors>Sam Park</Authors>
		<Description>Provides base functionalities for working with LCF framework</Description>
		<PackageProjectUrl>https://github.com/SamParkLCF/Core</PackageProjectUrl>
		<RepositoryUrl>https://github.com/SamParkLCF/Core</RepositoryUrl>
		<RepositoryType>git</RepositoryType>
		<PackageTags>LCF;LCFFramework;ObjectBase;TelemetryObject;ObjectLifeTime;</PackageTags>
		<PackageLicenseFile>LICENSE</PackageLicenseFile>
		<Title>LCF Core lib</Title>
	</PropertyGroup>

	<ItemGroup>
		<PackageReference Include="Humanizer.Core" Version="2.11.10" />
		<PackageReference Include="Newtonsoft.Json" Version="13.0.1" />
		<PackageReference Include="Serilog" Version="2.10.0" />
	</ItemGroup>

	<ItemGroup>
		<None Include="..\..\..\LICENSE">
			<Pack>True</Pack>
			<PackagePath></PackagePath>
		</None>
	</ItemGroup>

</Project>
